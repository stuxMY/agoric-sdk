{
  "name": "@agoric/telemetry",
  "version": "0.1.1",
  "description": "Agoric's telemetry implementation",
  "type": "module",
  "repository": "https://github.com/Agoric/agoric-sdk",
  "main": "./src/index.js",
  "scripts": {
    "build": "exit 0",
    "test": "ava",
    "test:c8": "c8 $C8_OPTIONS ava --config=ava-nesm.config.js",
    "test:xs": "exit 0",
    "lint-fix": "yarn lint:eslint --fix",
    "lint": "run-s --continue-on-error lint:*",
    "lint:types": "tsc --maxNodeModuleJsDepth 4 -p jsconfig.json",
    "lint:eslint": "eslint '**/*.js'"
  },
  "bin": {
    "frcat": "./src/frcat-entrypoint.js"
  },
  "keywords": [],
  "author": "Agoric",
  "license": "Apache-2.0",
  "dependencies": {
    "@agoric/store": "^0.6.10",
    "@endo/init": "^1.0.0",
    "@opentelemetry/api": "^1.0.0",
    "@opentelemetry/exporter-prometheus": "^0.27.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.27.0",
    "@opentelemetry/resources": "^1.0.1",
    "@opentelemetry/sdk-metrics-base": "^0.27.0",
    "@opentelemetry/sdk-trace-base": "^1.0.1",
    "@opentelemetry/semantic-conventions": "^1.0.1",
    "anylogger": "^0.21.0",
    "better-sqlite3": "^7.4.1",
    "bufferfromfile": "agoric-labs/BufferFromFile#Agoric-built",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "@endo/lockdown": "^0.1.9",
    "@endo/ses-ava": "^0.2.21",
    "ava": "^3.12.1",
    "c8": "^7.7.2",
    "tmp": "^0.2.1"
  },
  "eslintConfig": {
    "extends": [
      "@agoric"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "timeout": "20m"
  }
}
